{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "web": {
      "development": {
        "local": 3000,
        "fallback": "workcraft.ai"
      }
    },
    "docs": {
      "development": {
        "local": 3001
      },
      "routing": [
        {
          "group": "docs",
          "paths": ["/docs", "/docs/:path*"]
        }
      ]
    }
  }
}
